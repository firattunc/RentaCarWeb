</html>
<script>
    $(function () {

        var dizi = [];
        var id;
        var currentdate = new Date();
        var datetime = currentdate.getFullYear() + "-"
            + (currentdate.getMonth() + 1) + "-"
            + currentdate.getDate() + " "
            + currentdate.getHours() + ":"
            + currentdate.getMinutes();
        function listIstek() {
            $.ajax({
                type: "GET",
                url: "http://localhost:25667/api/Kiralama",
                dataType: "JSON",
                success: function (data) {

                    console.log(data);
                    $.each(data.Data, function (index, val) {


                        $("#listIstek").append(' <div class="col-md-12"> <div id="aracAdi" class="col-md-3">' + val.Arac.aracAdi + '</div><div class="col-md-3">' + val.kiralamaTarihi + '</div><div class="col-md-3">' + val.Musteri.Id + '</div></div>');



                    })
                }

            })
        }
        listIstek();
        $("#btnOnay").click(function () {
            $.ajax({
                type: "GET",
                url: "http://localhost:25667/api/Kiralama",
                dataType: "JSON",               
                success: function (data) {

                    console.log(data);
                    $.each(data.Data, function (index, val) {

                        $.ajax({
                            type: "PUT",
                            url: "http://localhost:25667/api/Kiralama/" + val.Id,
                            dataType: "JSON",
                            data: {
                                "Calisan": { Id: 93 }, "kiralamaTarihi": datetime
                            },
                            success: function (data) {

                                location.reload();
                               
                                
                            }

                        })
                        
                        return false;
                    })
                   
                }
                
            })
           
            


        });
        $("#btnRed").click(function myfunction() {

            $.ajax({
                type: "GET",
                url: "http://localhost:25667/api/Kiralama",
                dataType: "JSON",
                success: function (data) {

                    console.log(data);
                    $.each(data.Data, function (index, val) {

                        $.ajax({
                            type: "DELETE",
                            url: "http://localhost:25667/api/Kiralama/" + val.Id,
                            dataType: "JSON",
                            success: function (data) {

                                location.reload();

                            }

                        })

                        return false;
                    })

                }

            })


        });


    });
</script>